{
  "name": "moamahfouz/laravel-functions-extender",
  "description": "Some common functions I always use for arabic laravel projects",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Moahmmed Mahfouz",
      "email": "moa.mahfouz@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Moamahfouz\\LaravelFunctionsExtender\\": "src/",
      "Moamahfouz\\LaravelFunctionsExtender\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Moamahfouz\\LaravelFunctionsExtenderServiceProvider"
      ],
      "aliases": {
        "LaravelFunctionsExtender": "Moamahfouz\\LaravelFunctionsExtender\\Facades\\LaravelFunctionsExtender"
      }
    }
  },
  "scripts": {
    "test": "phpunit tests/"
  },
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0",
    "nesbot/carbon": "^2.72"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  }
}
